{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "model_id": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "seats": {
      "type": "integer"
    },
    "doors": {
      "type": "integer"
    },
    "transmission": {
      "type": "string"
    },
    "fuel_type": {
      "type": "enumeration",
      "enum": [
        "gasoline",
        "diesel"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "models"
    },
    "vehicles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vehicle.vehicle",
      "inversedBy": "models"
    }
  }
}
